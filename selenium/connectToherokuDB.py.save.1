app = Flask(__name__)
engine = create_engine('postgres://XXXXXXX@XXXXcompute-1.amazonaws.com:XXX')
Base = declarative_base()
Base.metadata.bind = engine
DBSession = sessionmaker(bind=engine)
session = DBSession()
